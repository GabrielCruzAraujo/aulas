algoritmo "contageminteligente"
var
   I, F,  Maior, Menor: Inteiro
Inicio
   Escreva("Inicio: ")
   Leia (I)
   Escreva ("Fim: ")
   Leia (F)

   se (I < F) entao
   enquanto (I <= F) faca
       escreval (I)
       I <- I + 1
   fimenquanto
   senao
    enquanto (I >= F) faca
       escreval (I)
       I <- I - 1
    fimenquanto
    Fimse
  Fimalgoritmo

inicio
fimalgoritmo
